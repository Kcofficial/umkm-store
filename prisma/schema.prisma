generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Store {
  id          Int      @id @default(autoincrement())
  name        String
  slug        String   @unique
  description String?
  sheetUrl    String
  whatsapp    String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
