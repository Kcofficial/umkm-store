generator client { provider = "prisma-client-js" }
datasource db { provider = "sqlite"; url = "file:./dev.db" }

model Store {
  id          Int       @id @default(autoincrement())
  name        String
  slug        String    @unique
  description String?
  sheetUrl    String
  whatsapp    String?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
